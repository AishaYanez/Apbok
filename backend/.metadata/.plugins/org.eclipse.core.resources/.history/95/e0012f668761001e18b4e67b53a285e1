package com.aisha.appGallery.entity.dao;

import org.springframework.data.jpa.repository.Query;
import org.springframework.data.repository.CrudRepository;
import org.springframework.data.repository.query.Param;

import com.aisha.appGallery.entity.models.User;

public interface IUserDao extends CrudRepository<User,Long>{
	@Query("SELECT s.bicyclesInShop FROM Shop s WHERE s.cif = :cif")
	public User findAllBicyclesByShopCif(@Param("email") String email);
}
